package backend;

public class Burst {

	private BurstType type;
	public int time;

	public boolean isCPU() {
		// TODO Auto-generated method stub
		return false;
	}

	public boolean isIO() {
		return 0 <= type.number && type.number <= 2;
	}

	public int getRequestNumber() {
		return type.number;
		return 0;
	}

	public boolean isNotArrived() {
		// TODO Auto-generated method stub
		return false;
	}

	private enum BurstType {
		IO_0(0), IO_1(1), IO_(2), CPU(3), DEAD(4);
		private BurstType(int number) {
			this.number = number;
		}
		private int number;
	}
	
}
